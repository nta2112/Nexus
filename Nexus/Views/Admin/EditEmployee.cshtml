@model Employee

<h2>Employee Details</h2>
<div>
    <h4>Employee</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>@Model.EmployeeId</dd>

        <dt>Name</dt>
        <dd>@Model.Name</dd>

        <dt>Email</dt>
        <dd>@Model.Email</dd>

        <dt>Password</dt>
        <dd>@Model.Password</dd>

        <dt>Role Name</dt>
        <dd>@Model.RoleName</dd>
    </dl>
</div>
